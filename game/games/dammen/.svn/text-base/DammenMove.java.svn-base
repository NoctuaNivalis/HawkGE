import java.awt.Point;
import java.io.Serializable;

/**
 *
 * @author michaelkint
 */
public class DammenMove implements Serializable {
    
    private Point origin, destination;

    public DammenMove(Point origin, Point destination){
        this.origin = origin;
        this.destination = destination;
    }
    
    public Point getDestination() {
        return destination;
    }

    public Point getOrigin() {
        return origin;
    }    
}
