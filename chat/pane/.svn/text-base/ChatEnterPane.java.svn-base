package hawkge.chat.pane;

import hawkge.chat.area.ChatFieldArea;
import hawkge.chat.area.TeamChatFieldArea;
import java.awt.Dimension;
import javax.swing.JScrollPane;

/**
 *
 * @author Sels
 */
public class ChatEnterPane extends JScrollPane {

    /*
     * Maakt een ScrollPane aan voor het ChatFieldArea
     */
    public ChatEnterPane(ChatFieldArea chatenter) {
        super(chatenter);
        this.setMinimumSize(new Dimension(280, 50));
    }

    public ChatEnterPane(TeamChatFieldArea chatenter) {
        super(chatenter);
        this.setMinimumSize(new Dimension(280, 50));
    }
}
